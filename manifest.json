{
    "manifest_version": 3,
    "name": "PageMark",
    "version": "1.0",
    "description": "Save coding question links and explanations to an Excel sheet",
    "permissions": [
      "activeTab",
      "storage"
    ],
    "icons" :{
      "16": "icon1.png",
      "48": "icon1.png",
      "128": "icon1.png"
    },
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  